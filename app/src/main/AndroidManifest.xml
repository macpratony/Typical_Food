<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.typicalfood">
    <!-- DANDO PERMISO ACCESO A INTENET DE LA APP -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Main_Navigation_Drawer_Activity.NavigationDrawerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- con esta linea eliminamos el toolbar de nuestra aplicacion -->
        <activity android:name=".ScreenMainActivity">

        </activity>
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".RegistrarseActivity" />
        <activity android:name=".AutenticacionActivity"></activity>
        <activity android:name=".ProvinciasActivity" />
    </application>

</manifest>